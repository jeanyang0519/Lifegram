<!DOCTYPE html>
<html>
  <head>
    <title>Lifegram</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>


    <script type="text/javascript">
      window.signupPhoto = '<%= image_url('signup-photo.png') %>'
      window.logo = '<%= image_url('logo.png') %>'
      window.add = '<%= image_url('add.png') %>'
      window.icon = '<%= image_url('icon.png') %>'
      window.logout = '<%= image_url('logout.png') %>'
      window.setting = '<%= image_url('setting.png') %>'
      window.profile = '<%= image_url('profile.png') %>'
      window.polaroid = '<%= image_url('polaroid.png') %>'
      window.polaroidPhoto = '<%= image_url('polaroid-photo.png') %>'
      window.redlike = '<%= image_url('redlike.png') %>'
      window.comment = '<%= image_url('comment.png') %>'
      window.like = '<%= image_url('like.png') %>'
      window.option = '<%= image_url('option.png') %>'
      window.user = '<%= image_url('user.png') %>'
      window.grid = '<%= image_url('grid.png') %>'
      window.photos = '<%= image_url('photos.png') %>'
      window.menu = '<%= image_url('menu.png') %>'
      window.whitelike = '<%= image_url('whitelike.png') %>'
      window.whitecomment = '<%= image_url('whitecomment.png') %>'
      window.camera = '<%= image_url('camera.png') %>'
      window.policy = '<%= image_url('policy.png') %>'
      window.plane = '<%= image_url('plane.png') %>'
      window.instagram = '<%= image_url('instagram-color.png') %>'
    </script>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
